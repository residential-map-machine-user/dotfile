{
  "env": {
    "jquery": true,
      "browser": true,
      "es6": true
  },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "space-in-parens": ["error", "never"],
      "indent": ["error", 2],
      "space-before-blocks": "error",
      "space-before-function-paren": ["error", "never"]
    },
    "extends": "eslint:recommended"
}
